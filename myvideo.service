#TITLE# DRONE service myvideo
# /lib/systemd/system/myvideo.service
[Unit]
Description=myvideo
After=multi-user.target

[Service]
Type=forking
ExecStart=/usr/local/bin/manage_video.sh start
ExecStop=/usr/local/bin/manage_video.sh stop
#KillMode=process
#Restart=on-failure

[Install]
WantedBy=multi-user.target
